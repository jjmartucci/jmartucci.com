---
import {CollectionEntry} from "astro:content";
import PlantTag from "../PlantTag/PlantTag.astro";
import SlottedPreview from "./SlottedPreview.astro";
interface Props {
    note: CollectionEntry<"garden">
}
const {note} = Astro.props
---

<SlottedPreview
        created={note.data.created}
        modified={note.data.modified}
        slug={note.slug} image={note.data.image} type="note" title={note.data.title} description={note.data.description} tags={note.data.tags}>
</SlottedPreview>

<style>
    img {
        border-radius: 0;
    }
</style>
